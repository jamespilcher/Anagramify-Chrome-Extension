{
    "name": "Anagramify",
    "description": "Turns every word on web page into an anagram of itself.",
    "version": "1.0",
    "manifest_version": 3,
    "icons": {"128" : "128A.png"},
    "background": {
        "service_worker": "onclick.js"
        },
    "action" : {"default_title" : "Anagramify it, baby!"},
    "permissions": [
        "activeTab",
        "tabs",
        "scripting"
      ],
    "host_permissions": [
        "<all_urls>"
      ]
}